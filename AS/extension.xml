<?xml version="1.0" encoding="utf-8" ?><extension xmlns="http://ns.adobe.com/air/extension/18.0">    <id>eu.amodo.extensions.applicationupdater</id>    <versionNumber>0.0.1</versionNumber>    <platforms>        <platform name="Android-ARM">            <applicationDeployment>                <nativeLibrary>ApplicationUpdater.jar</nativeLibrary>                <initializer>eu.amodo.extensions.applicationupdater.ExtensionActivity</initializer>                 <finalizer>eu.amodo.extensions.applicationupdater.ExtensionActivity</finalizer>            </applicationDeployment>        </platform>        <platform name="iPhone-ARM">            <applicationDeployment>                <nativeLibrary>libApplicationUpdater.a</nativeLibrary>                <initializer>ApplicationUpdaterExtInitializer</initializer>                <finalizer>ApplicationUpdaterExtFinalizer</finalizer>            </applicationDeployment>        </platform>		<platform name="default">            <applicationDeployment/>        </platform>    </platforms></extension>